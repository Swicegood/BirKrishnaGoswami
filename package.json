{
  "name": "bkgonestopshop",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "postinstall": "patch-package",
    "build:android": "node scripts/prepare-android-build.js && eas build --platform android --profile debug-preview --local && node scripts/restore-package.js",
    "build:release:android:apk": "node scripts/prepare-android-build.js && eas build --platform android --profile release-apk --local && node scripts/restore-package.js",
    "build:ios": "node scripts/prepare-ios-build.js && eas build --platform ios --profile debug-preview --local && node scripts/restore-package.js",
    "build:android:release": "node scripts/prepare-android-build.js && eas build --platform android --profile release --local && node scripts/restore-package.js",
    "build:ios:release": "node scripts/prepare-ios-build.js && eas build --platform ios --profile release --local && node scripts/restore-package.js",
    "prepare:android": "node scripts/prepare-android-build.js",
    "prepare:ios": "node scripts/prepare-ios-build.js",
    "restore": "node scripts/restore-package.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-track-player",
          "react-native-snap-carousel",
          "react-native-swiper",
          "cors",
          "deprecated-react-native-prop-types",
          "expo-server-sdk",
          "firebase",
          "metro",
          "postinstall-postinstall",
          "react-native-image-gallery",
          "react-native-vector-icons",
          "react-native-web-webview"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@expo/server": "^0.4.2",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/messaging": "^19.0.1",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.0.0",
    "@types/axios": "^0.14.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "deprecated-react-native-prop-types": "^5.0.0",
    "expo": "^53.0.0",
    "expo-av": "~15.1.7",
    "expo-background-fetch": "^13.1.6",
    "expo-build-properties": "~0.14.8",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-intent-launcher": "~12.1.5",
    "expo-linking": "~7.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.6",
    "expo-screen-orientation": "~8.1.7",
    "expo-server-sdk": "^3.7.0",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "firebase": "^10.8.0",
    "lodash": "^4.17.21",
    "metro": "^0.82.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-device-info": "^11.1.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-image-gallery": "^2.1.5",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.15.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "15.11.2",
    "react-native-swiper": "^1.6.0",
    "react-native-track-player": "^4.1.2",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.20.0",
    "react-native-web-webview": "^1.0.2",
    "react-native-webview": "13.13.5",
    "react-native-youtube-iframe": "^2.3.0",
    "react-native-worklets": "0.3.0",
    "shaka-player": "^4.16.9"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-transform-optional-chaining": "^7.24.0",
    "@babel/plugin-transform-template-literals": "^7.24.0",
    "@react-native-community/cli": "^20.0.1",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "~19.0.10",
    "@types/react-native-image-gallery": "^2.1.5",
    "@types/react-native-snap-carousel": "^3.8.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "jest": "^29.4.0",
    "jest-expo": "~53.0.10",
    "patch-package": "^8.0.0",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  },
  "private": true
}